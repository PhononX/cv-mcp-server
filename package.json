{
  "name": "cv-mcp-server",
  "version": "1.0.0",
  "main": "index.js",
  "bin": {
    "cv-mcp-server": "./build/index.js"
  },
  "scripts": {
    "build": "rm -rf build && tsc && chmod 755 build/stdio.js",
    "start": "node build/index.js",
    "pre": "npm run build",
    "mcp:inspector": "npx --env-file=.env @modelcontextprotocol/inspector node build/stdio.js",
    "dev": "dotenv -e .env -- node build/index.js",
    "debug": "dotenv -e .env -- nodemon --watch src --ext ts --exec \"node --loader ts-node/esm\" src/server.ts",
    "debug:stdio": "dotenv -e .env -- nodemon --watch src --ext ts --exec \"node --loader ts-node/esm\" src/stdio.ts",
    "auto:build": "nodemon --watch src --ext ts --exec npm run build",
    "pregenerate:api": "tsc src/utils/zod-transformer.ts --outDir build/utils && mv build/utils/zod-transformer.js build/utils/zod-transformer.cjs",
    "generate:api": "npm run pregenerate:api && orval --config orval.config.ts"
  },
  "files": [
    "build"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "openapi-zod-client": "^1.18.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@orval/zod": "^7.9.0",
    "@types/handlebars": "^4.0.40",
    "@types/node": "^22.15.29",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.2.0",
    "nodemon": "^3.1.10",
    "orval": "^7.9.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "zod": "^3.25.58"
  }
}
